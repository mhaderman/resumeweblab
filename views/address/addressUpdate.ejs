<%- include("../includes/header.ejs", {title: "Update Address Id " + address.address_id}); %>

<h1>Update Address Id <%= address.address_id%> </h1>
<form action="/address/update" method="get">
    <input type="hidden" id="address_id" name="address_id" value="<%= address.address_id %>">
    Address Street: <input id="street" name="street" type="text" value="<%= address.street %>"/>
    <br />
    Address Zipcode: <input id="zip_code" name="zip_code" type="text" value="<%= address.zip_code %>"/>
    <br />
    <br />

    <input type="submit" />

</form>

<%- include("../includes/footer.ejs"); %>

</html>