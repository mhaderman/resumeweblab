<%- include("../includes/header.ejs", {title:"Resume - View All"}); %>

<h1>Resume's</h1>
        <a href="/resume/add/selectuser">Add New Resume</a> <br />

        <br />

        <table border="1">
            <tr><th>First Name</th><th>Last Name</th><th>Resume Name</th></tr>
            <% for(var i=0; result.length > i; i++ ) { %>
            <tr>
                <td><a href="/resume/?resume_id=<%= result[i].resume_id %>"><%= result[i].first_name %></a></td>
                <td><a href="/resume/?resume_id=<%= result[i].resume_id %>"><%= result[i].last_name %></a></td>
                <td><a href="/resume/?resume_id=<%= result[i].resume_id %>"><%= result[i].resume_name %></a></td>
            </tr>

            <% } %>
        </table>
<%- include("../includes/footer.ejs"); %>